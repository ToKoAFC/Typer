@model Typer.ViewModels.Views.AdminSeason.VMAdminSeasonIndex

@{
    ViewBag.Title = "Index";
}

<div class="row page-header">
    <div class="col-md-11">
        <h4 class="headline-title">Zarządzaj sezonami</h4>
    </div>
    <div class="col-md-1">
        @Html.ActionLink("Dodaj sezon", "AddNewSeason", "AdminSeason", null, new { @class = "btn btn-default" })
    </div>
</div>
<div class="panel panel-default">
    <div class="panel-body">
        @foreach (var season in Model.Seasons)
        {
            <div class="row">
                <p>@(season.StartYear + "/" + season.EndYear)</p>
            </div>
        }
    </div>
</div>